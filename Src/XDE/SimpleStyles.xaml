                                    <Storyboard
										x:Uid="Storyboard_6">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_5"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="Rectangle">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_5"
