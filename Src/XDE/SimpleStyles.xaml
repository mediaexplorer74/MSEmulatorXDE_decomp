					TargetType="{x:Type Thumb}">
                    <Grid
						Name="Grid"
						x:Uid="Grid_2"
						Margin="0,0,0,0">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup
								x:Uid="VisualStateGroup_2"
								x:Name="CommonStates">
                                <VisualState
									x:Uid="VisualState_5"
