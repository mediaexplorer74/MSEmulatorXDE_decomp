                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState
									x:Uid="VisualState_7"
									x:Name="Pressed">
                                    <Storyboard
										x:Uid="Storyboard_8">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_7"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="Rectangle">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_7"
												KeyTime="0"
												Value="0.4" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState
									x:Uid="VisualState_8"
									x:Name="Disabled">
                                    <Storyboard
										x:Uid="Storyboard_9">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_8"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="Rectangle">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_8"
