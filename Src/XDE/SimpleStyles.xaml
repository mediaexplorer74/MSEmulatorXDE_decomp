                                    <Storyboard
										x:Uid="Storyboard_7">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_6"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="Rectangle">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_6"
												KeyTime="0"
