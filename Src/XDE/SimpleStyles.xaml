    <Style
		TargetType="{x:Type xdecommon:BrowserHyperlink}"
		x:Key="{x:Type xdecommon:BrowserHyperlink}"
        >
        <Setter
			Property="Foreground"
			Value="{StaticResource Brush_Text_Foreground}" />
        <Setter
			Property="Inline.TextDecorations"
			Value="Underline" />
        <Style.Triggers>
            <Trigger
				Property="ContentElement.IsMouseOver"
				Value="True">
                <Setter
					Property="Foreground"
					Value="#FFFFFF00" />
            </Trigger>
            <Trigger
				Property="ContentElement.IsEnabled"
				Value="False">
                <Setter
					Property="Foreground"
					Value="{DynamicResource SystemColors.GrayTextBrushKey}" />
            </Trigger>
            <Trigger
				Property="ContentElement.IsEnabled"
				Value="True">
                <Setter
					Property="FrameworkContentElement.Cursor"
					Value="Hand" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style
		x:Uid="Style_2"
		TargetType="{x:Type RepeatButton}"
		BasedOn="{x:Null}"
		x:Key="General_RepeatButton_Style">
        <Setter
			x:Uid="Setter_2"
			Property="Background">
            <Setter.Value>
                <LinearGradientBrush
					x:Uid="LinearGradientBrush_1"
					EndPoint="0,1"
					StartPoint="0,0">
                    <GradientStop
						x:Uid="GradientStop_1"
						Color="#EEE"
						Offset="0.0" />
                    <GradientStop
						x:Uid="GradientStop_2"
						Color="#CCC"
						Offset="1.0" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter
			x:Uid="Setter_3"
			Property="BorderBrush">
            <Setter.Value>
                <LinearGradientBrush
					x:Uid="LinearGradientBrush_2"
					EndPoint="0,1"
					StartPoint="0,0">
                    <GradientStop
						x:Uid="GradientStop_3"
						Color="#CCC"
						Offset="0.0" />
                    <GradientStop
						x:Uid="GradientStop_4"
						Color="#444"
						Offset="1.0" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter
			x:Uid="Setter_4"
			Property="Template">
            <Setter.Value>
                <ControlTemplate
					x:Uid="ControlTemplate_2"
					TargetType="{x:Type RepeatButton}">
                    <Grid
						Name="grid"
						x:Uid="Grid_39"
						Opacity="0.65">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup
								x:Uid="VisualStateGroup_1"
								x:Name="CommonStates">
                                <VisualState
									x:Uid="VisualState_1"
									x:Name="Normal">
                                    <Storyboard
										x:Uid="Storyboard_2">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_1"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="grid">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_1"
												KeyTime="0"
												Value="0.4" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState
									x:Uid="VisualState_2"
									x:Name="MouseOver">
                                    <Storyboard
										x:Uid="Storyboard_3">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_2"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="grid">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_2"
												KeyTime="0"
												Value="0.9" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState
									x:Uid="VisualState_3"
									x:Name="Pressed">
                                    <Storyboard
										x:Uid="Storyboard_4">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_3"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="grid">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_3"
												KeyTime="0"
												Value="0.4" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState
									x:Uid="VisualState_4"
									x:Name="Disabled">
                                    <Storyboard
										x:Uid="Storyboard_5">
                                        <DoubleAnimationUsingKeyFrames
											x:Uid="DoubleAnimationUsingKeyFrames_4"
											Storyboard.TargetProperty="(UIElement.Opacity)"
											Storyboard.TargetName="grid">
                                            <EasingDoubleKeyFrame
												x:Uid="EasingDoubleKeyFrame_4"
												KeyTime="0"
												Value="0.2" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border
							Name="Border"
							x:Uid="Border_1"
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}" />
                        <ContentPresenter
							Name="ContentPresenter"
							x:Uid="ContentPresenter"
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							Content="{TemplateBinding Content}"
							ContentTemplate="{TemplateBinding ContentTemplate}"
							ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
