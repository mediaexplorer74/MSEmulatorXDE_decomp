    <Style
		x:Uid="Style_4"
		TargetType="{x:Type RepeatButton}"
		x:Key="ScrollRepeatButton_Style">
        <Setter
			x:Uid="Setter_6"
			Property="Background"
			Value="#00FFFFFF" />
        <Setter
			x:Uid="Setter_7"
			Property="BorderBrush"
			Value="#00FFFFFF" />
        <Setter
			x:Uid="Setter_8"
			Property="IsTabStop"
			Value="False" />
        <Setter
			x:Uid="Setter_9"
			Property="Focusable"
			Value="False" />
        <Setter
			x:Uid="Setter_10"
			Property="Template">
            <Setter.Value>
                <ControlTemplate
					x:Uid="ControlTemplate_4"
					TargetType="{x:Type RepeatButton}">
                    <Grid
						x:Uid="Grid_3">
                        <Rectangle
							x:Uid="Rectangle_1"
							Fill="{TemplateBinding Background}"
							Stroke="{TemplateBinding BorderBrush}"
							StrokeThickness="{TemplateBinding BorderThickness}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <DataTrigger
				Value="True"
				Binding="{Binding Path=(SystemParameters.HighContrast)}">
                <Setter
					x:Uid="Setter_6"
					Property="Background"
					Value="{DynamicResource SystemColors.ControlBrushKey}" />
                <Setter
					x:Uid="Setter_7"
					Property="BorderBrush"
					Value="{DynamicResource SystemColors.ControlTextBrushKey}" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition
						Value="True"
						Binding="{Binding Path=(SystemParameters.HighContrast)}" />
                    <Condition
						Value="true"
						Binding="{Binding Path=IsSelected, RelativeSource={RelativeSource Self}}" />
                </MultiDataTrigger.Conditions>
                <Setter
					x:Uid="Setter_6"
					Property="Background"
					Value="{DynamicResource SystemColors.HighlightBrushKey}" />
                <Setter
					x:Uid="Setter_7"
					Property="BorderBrush"
					Value="{DynamicResource SystemColors.HighlightTextBrushKey}" />
            </MultiDataTrigger>
        </Style.Triggers>
    </Style>
    <Style
		x:Uid="Style_5"
		TargetType="{x:Type ScrollBar}"
		x:Key="ScrollBar_Style">
        <Setter
			x:Uid="Setter_11"
			Property="Stylus.IsFlicksEnabled"
			Value="False" />
        <Setter
			x:Uid="Setter_12"
			Property="Foreground"
			Value="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" />
        <Setter
			x:Uid="Setter_13"
			Property="Template">
            <Setter.Value>
                <ControlTemplate
					x:Uid="ControlTemplate_5"
					TargetType="{x:Type ScrollBar}">
                    <Grid
						Name="GridRoot"
						x:Uid="GridRoot"
						Width="{StaticResource {x:Static SystemParameters.VerticalScrollBarWidthKey}}"
						Background="{x:Null}">
                        <Grid.RowDefinitions>
                            <RowDefinition
								x:Uid="RowDefinition_1"
								MaxHeight="18" />
                            <RowDefinition
								x:Uid="RowDefinition_2"
								Height="0.00001*" />
                            <RowDefinition
								x:Uid="RowDefinition_3"
								MaxHeight="18" />
                        </Grid.RowDefinitions>
                        <RepeatButton
							Name="DecreaseRepeat"
							x:Uid="DecreaseRepeat"
							Command="ScrollBar.LineUpCommand"
							Margin="0,-2,0,0"
							Style="{StaticResource General_RepeatButton_Style}"
							Background="{x:Null}"
							BorderBrush="{x:Null}">
                            <Grid
								x:Uid="Grid_4"
								Background="#02000000">
                                <Path
									Name="DecreaseArrow"
									x:Uid="DecreaseArrow"
									StrokeThickness="1"
									Data="M0,4 L8,4 L4,0">
                                    <Shape.Fill>
                                        <SolidColorBrush
											x:Uid="SolidColorBrush_17"
											Color="{StaticResource Color_ForegroundTextcolor}" />
                                    </Shape.Fill>
                                    <Shape.Stroke>
                                        <SolidColorBrush
											x:Uid="SolidColorBrush_18"
											Color="{StaticResource Color_ForegroundTextcolor}" />
                                    </Shape.Stroke>
                                </Path>
                            </Grid>
                        </RepeatButton>
                        <Track
							Name="PART_Track"
							x:Uid="PART_Track"
							Grid.Row="1"
							Orientation="Vertical"
							IsDirectionReversed="True">
                            <Track.Thumb>
                                <Thumb
									x:Uid="Thumb_1"
									Style="{StaticResource Thumb_Style}" />
                            </Track.Thumb>
                            <Track.IncreaseRepeatButton>
                                <RepeatButton
									Name="PageUp"
									x:Uid="PageUp"
									Command="ScrollBar.PageDownCommand"
									Style="{StaticResource ScrollRepeatButton_Style}" />
                            </Track.IncreaseRepeatButton>
                            <Track.DecreaseRepeatButton>
                                <RepeatButton
									Name="PageDown"
									x:Uid="PageDown"
									Command="ScrollBar.PageUpCommand"
									Style="{StaticResource ScrollRepeatButton_Style}" />
                            </Track.DecreaseRepeatButton>
                        </Track>
                        <RepeatButton
							Name="IncreaseRepeat"
							x:Uid="IncreaseRepeat"
							Grid.Row="2"
							Command="ScrollBar.LineDownCommand"
							Margin="0,0,0,-2"
							Style="{StaticResource General_RepeatButton_Style}"
							Background="{x:Null}"
							BorderBrush="{x:Null}">
                            <Grid
								x:Uid="Grid_5"
								Background="#02000000">
                                <Path
									Name="IncreaseArrow"
									x:Uid="IncreaseArrow"
									StrokeThickness="1"
									Data="M0,0 L4,4 L8,0">
                                    <Shape.Fill>
                                        <SolidColorBrush
											x:Uid="SolidColorBrush_19"
											Color="{StaticResource Color_ForegroundTextcolor}" />
                                    </Shape.Fill>
                                    <Shape.Stroke>
                                        <SolidColorBrush
											x:Uid="SolidColorBrush_20"
											Color="{StaticResource Color_ForegroundTextcolor}" />
                                    </Shape.Stroke>
                                </Path>
                            </Grid>
                        </RepeatButton>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger
							x:Uid="Trigger_5"
							Property="ScrollBar.Orientation"
							Value="Horizontal">
                            <Setter
								x:Uid="Setter_14"
								TargetName="GridRoot"
								Property="FrameworkElement.LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform
										x:Uid="RotateTransform_1"
										Angle="-90" />
                                </Setter.Value>
                            </Setter>
                            <Setter
								x:Uid="Setter_15"
								TargetName="PART_Track"
								Property="Orientation"
								Value="Vertical" />
                            <Setter
								x:Uid="Setter_16"
								TargetName="DecreaseRepeat"
								Property="Command"
								Value="ScrollBar.LineLeftCommand" />
                            <Setter
								x:Uid="Setter_17"
								TargetName="IncreaseRepeat"
								Property="Command"
								Value="ScrollBar.LineRightCommand" />
                            <Setter
								x:Uid="Setter_18"
								TargetName="PageDown"
								Property="Command"
								Value="ScrollBar.PageLeftCommand" />
                            <Setter
								x:Uid="Setter_19"
								TargetName="PageUp"
								Property="Command"
								Value="ScrollBar.PageRightCommand" />
                        </Trigger>
                        <DataTrigger
							Value="True"
							Binding="{Binding Path=(SystemParameters.HighContrast)}">
                            <Setter
								x:Uid="Setter_12"
								Property="Foreground"
								Value="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" />
                            <Setter
								TargetName="IncreaseArrow"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Fill" />
                            <Setter
								TargetName="DecreaseArrow"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Fill" />
                            <Setter
								TargetName="GridRoot"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Background" />
                        </DataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition
									Value="True"
									Binding="{Binding Path=(SystemParameters.HighContrast)}" />
                                <Condition
									Value="true"
									Binding="{Binding Path=IsSelected, RelativeSource={RelativeSource Self}}" />
                            </MultiDataTrigger.Conditions>
                            <Setter
								x:Uid="Setter_12"
								Property="Foreground"
								Value="{StaticResource {x:Static SystemColors.HighlightTextBrushKey}}" />
                            <Setter
								TargetName="IncreaseArrow"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Fill" />
                            <Setter
								TargetName="DecreaseArrow"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Fill" />
                            <Setter
								TargetName="GridRoot"
								Value="{DynamicResource SystemColors.ControlTextBrushKey}"
								Property="Background" />
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <DataTrigger
				Value="True"
				Binding="{Binding Path=(SystemParameters.HighContrast)}">
                <Setter
					x:Uid="Setter_12"
					Property="Foreground"
					Value="{DynamicResource SystemColors.ControlTextBrushKey}" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition
						Value="True"
						Binding="{Binding Path=(SystemParameters.HighContrast)}" />
                    <Condition
						Value="true"
						Binding="{Binding Path=IsSelected, RelativeSource={RelativeSource Self}}" />
                </MultiDataTrigger.Conditions>
                <Setter
					x:Uid="Setter_12"
					Property="Background"
					Value="{DynamicResource SystemColors.HighlightBrushKey}" />
            </MultiDataTrigger>
