							Height="Auto"
							RadiusX="4"
							RadiusY="4"
							StrokeThickness="0"
							Margin="4,0"
							Opacity="0.4"
							Fill="{StaticResource Brush_Text_Foreground}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger
							x:Uid="Trigger_1"
							Property="UIElement.IsFocused"
							Value="True" />
                        <Trigger
							x:Uid="Trigger_2"
							Property="UIElement.IsMouseOver"
							Value="True" />
                        <Trigger
							x:Uid="Trigger_3"
							Property="IsEnabled"
							Value="False" />
                        <Trigger
							x:Uid="Trigger_4"
							Property="Thumb.IsDragging"
							Value="True" />
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
